{
  "name": "@pluginjs/shares",
  "description": "A flexible modern shares js plugin.",
  "license": "GPL-3.0",
  "author": "Creation Studio Limited",
  "homepage": "https://pluginjs.com",
  "repository": {
    "url": "git@github.com:pluginjs/pluginjs.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pluginjs/pluginjs/issues"
  },
  "version": "0.1.3",
  "category": "ui",
  "main": "dist/shares.common.js",
  "module": "dist/shares.esm.js",
  "umd": "dist/shares.js",
  "unpkg": "dist/shares.js",
  "jsdelivr": "dist/shares.js",
  "source": "src/main.js",
  "style": "dist/shares.css",
  "sass": "src/css/shares.scss",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run build:js && npm run build:scss",
    "build:js": "plugin script build-js",
    "build:md": "plugin script build-md",
    "build:scss": "plugin script build-scss",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint ./src/**/*.js --fix",
    "lint:scss": "stylelint ./src/**/*.scss --fix",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "@pluginjs/classes": "^0.8.3",
    "@pluginjs/component": "^0.8.3",
    "@pluginjs/decorator": "^0.8.3",
    "@pluginjs/dom": "^0.8.3",
    "@pluginjs/events": "^0.8.3",
    "@pluginjs/template": "^0.8.3",
    "@pluginjs/tooltip": "^0.8.3",
    "@pluginjs/utils": "^0.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@pluginjs/browserslist-config": "^1.3.0",
    "@pluginjs/cli": "^0.8.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "babel-jest": "*",
    "jest": "*",
    "jest-extended": "*",
    "rename": "^1.0.4",
    "rollup": "^2.10.0",
    "rollup-plugin-babel": "*",
    "rollup-plugin-terser": "*"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "jest-extended",
    "verbose": true,
    "testURL": "http://localhost",
    "testPathIgnorePatterns": [
      "fixtures"
    ]
  },
  "browserslist": [
    "extends @pluginjs/browserslist-config"
  ]
}
